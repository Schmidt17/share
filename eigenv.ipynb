    "import numpy as np\n",
    "\n",
    "matrix = np.matrix('6 -11 6; 1 0 0; 0 1 0')\n",
    "\n",
    "# Berechne die Eigenwerte von matrix\n",
    "eigenwerte, einheitsEVs = np.linalg.eig(matrix)\n",
    "\n",
    "print(\"Eigenwerte:\", eigenwerte)\n",
    "print(\"Einheits-Eigenvektoren:\")\n",
    "print(einheitsEVs)"
